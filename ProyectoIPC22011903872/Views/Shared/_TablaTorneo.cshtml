@model ProyectoIPC22011903872.Models.ViewModels.TorneoViewModel

@{
    Layout = Request.IsAjaxRequest() ? null : "";
}
@{
    int x0 = 0;
    int x1 = 0;
    int x2 = 0;
    int x3 = 0;
    int y = 0;
    var EncuentroActual = new ProyectoIPC22011903872.Models.ViewModels.Encuentro();
    var FaseActual = new ProyectoIPC22011903872.Models.ViewModels.Fase();
    var partidaActual = new ProyectoIPC22011903872.Models.ViewModels.PartidaViewModel();
    foreach (var Fase in Model.Fases)
    {
        var ac = false;
        foreach (var encuentro in Fase.Encuentros)
        {
            if (encuentro.Ganador == "" || encuentro.Ganador==null)
            {
                FaseActual = Fase;
                encuentro.actual = "actual";
                EncuentroActual = encuentro;
                ac = true;
                foreach (var partida in encuentro.Partidas.partidas)
                {
                    if (partida.terminado != "si")
                    {
                        partidaActual = partida;
                        break;
                    }
                }
                break;
            }
            else
            {
                encuentro.actual = "";
            }
        }
        if (ac)
        {
            break;
        }
    }
}

@if (ViewBag.empate != null)
{
    <div class="modal fade" id="exampleModalCenter" aria-labelledby="exampleModalCenterTitle">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header bg-verde text-white font-weight-bold">
                    <h5 class="modal-title text-center w-100" id="exampleModalLongTitle">Saltar Turno</h5>

                </div>
                <div class="modal-body">
                    <p class="w-100 text-center">El Encuentro quedo en empate, se generara una nueva partida</p>
                    <div class="row">
                        <div class="col col-6">
                            <label>Jugador @EncuentroActual.Equipo1</label>
                            <select class="form-control" id="jugador1">
                                @foreach (var equipo in Model.Equipos)
                                {
                                    if (equipo.nombre == EncuentroActual.Equipo1)
                                    {
                                        foreach (var jug in equipo.jugadores)
                                        {
                                            <option value="@jug">@jug</option>
                                        }
                                        break;
                                    }
                                }
                            </select>
                        </div>
                        <div class="col col-6">
                            <label>Jugador @EncuentroActual.Equipo2</label>
                            <select class="form-control" id="jugador2">
                                @foreach (var equipo in Model.Equipos)
                                {
                                    if (equipo.nombre == EncuentroActual.Equipo2)
                                    {
                                        foreach (var jug in equipo.jugadores)
                                        {
                                            <option value="@jug">@jug</option>
                                        }
                                        break;
                                    }
                                }
                            </select>
                        </div>
                    </div>
                    <button class="btn btn-dark btn-block mt-1" onclick="EmpateE('@Model.codigo','@EncuentroActual.codigo','@FaseActual.nombre','@partidaActual.nombre')">Definir Encuentro</button>
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>
}
@if (ViewBag.Terminado != null)
{
    <div class="modal fade" id="exampleModalCenter" aria-labelledby="exampleModalCenterTitle">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header bg-verde text-white font-weight-bold">
                    <h5 class="modal-title text-center w-100" id="exampleModalLongTitle">Partida Terminada</h5>

                </div>
                <div class="modal-body">
                    @{ 
                        var equipoGanador = new ProyectoIPC22011903872.Models.ViewModels.Equipo();
                        foreach (var equ in Model.Equipos)
                        {
                            if (equ.nombre == Model.Ganador)
                            {
                                equipoGanador = equ;
                                break;
                            }
                        }
                        var ganador = "El equipo ganador es: "+Model.Ganador;
                        var ganadorP = "Punteo total de: " + equipoGanador.puntaje;
                    }
                    <p class="w-100 text-center">@ganador</p>
                    <p class="w-100 text-center">@ganadorP</p>
                    @Html.ActionLink("Aceptar", "Index", "Partida", new { @class = "btn btn-dark btn-block redondear" })
                </div>
                <div class="modal-footer">


                </div>
            </div>
        </div>
    </div>
}

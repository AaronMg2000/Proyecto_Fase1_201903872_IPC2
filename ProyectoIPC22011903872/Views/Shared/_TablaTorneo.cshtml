                                </div>
                            </div>
                        </div>
                        <div class="col col-12" style="height:12.5%;">
                            <div class="row align-content-center justify-content-end h-100">
                                <div class="col col-4 h-100">
                                    <div class="row align-content-start justify-content-end h-100">
                                        <div class="col col-12 h-50" style="border-bottom:solid 2px orange;border-left:solid 2px orange;">
                                            @if (Model.Fases.Count() > y)
                                            {
                                                @Model.Fases[y].Encuentros[x0].punteo2
                                            }
                                        </div>
                                    </div>
                                </div>
                                <div class="col col-8 h-100">
                                    <div class="row justify-content-center h-100 align-content-center">
                                        @if (Model.Fases.Count() > y)
                                        {
                                            GG1 = Model.Fases[y].Encuentros[x0].resultado2;
                                            acctual = Model.Fases[y].Encuentros[x0].actual;
                                        }
                                        <div class="col bg-verde col-12 @GG1 @acctual" style="height:85%;">
                                            @if (Model.Fases.Count() > y)
                                            {
                                                <div class="row h-100 justify-content-center align-content-center">
                                                    <div class="col col-12 text-white font-weight-bold">
                                                        @Model.Fases[y].Encuentros[x0].Equipo2
                                                    </div>
                                                </div>
                                                x0++;
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
    <div class="col col-12 bg-negro-90" style="height:25%">
        <div class="row justify-content-center align-content-center h-100">
            <div class="col bg-dark col-8" style="height:80%">
                <div class="row justify-content-around align-content-around h-100">
                    <div class="col col-3 w-100">
                        <div class="row h-100">
                            <div class="col col-12 bg-verde text-center text-white font-weight-bold" style="height:50%">
                                <div class="row h-100 align-items-center">
                                    <div class="col col-12">
                                        @EncuentroActual.Equipo1
                                        <div class="w-100"></div>
                                        @partidaActual.jugador1
                                    </div>
                                </div>
                            </div>
                            <div class="col col-12 p-0">
                                <input type="text" class="form-control text-center" @*onkeyup="detectar()"*@ name="punteo1" id="punteo1" value="0" placeholder="Punteo Equipo" />
                            </div>
                        </div>
                    </div>
                    <div class="col col-4" style="height:80%">
                        <form action="/Torneo/Partida" method="get" class="mb-1">
                            <input type="text" name="nombre" value="@Model.codigo" class="d-none" />
                            <input type="text" name="encuentro" value="@EncuentroActual.codigo" class="d-none" />
                            <input type="text" name="fase" value="@FaseActual.nombre" class="d-none" />
                            <input type="text" name="part" value="@partidaActual.nombre" class="d-none" />
                            <input type="submit" class="btn btt-yellow btn-block" value="Iniciar Encuentro">
                        </form>
                        <input class="btn btt-n2 btn-block" type="button" onclick="Definir('@Model.codigo','@EncuentroActual.codigo','@FaseActual.nombre','@partidaActual.nombre')" value="Definir Ganador">
                        <p class="text-center text-white font-weight-bold mt-1 h5">@Model.Nombre</p>
                    </div>
                    <div class="col col-3 w-100">
                        <div class="row h-100">
                            <div class="col col-12 bg-verde text-center text-white font-weight-bold" style="height:50%">
                                <div class="row h-100 align-items-center">
                                    <div class="col col-12">
                                        @EncuentroActual.Equipo2
                                        <div class="w-100"></div>
                                        @partidaActual.jugador2
                                    </div>
                                </div>
                            </div>
                            <div class="col col-12 p-0">
                                <input type="text" class="form-control text-center"@*onkeyup="detectar()"*@ name="punteo2" id="punteo2" value="0" placeholder="Punteo Equipo" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
